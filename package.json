{
  "name": "chrome-offline-indexer",
  "version": "1.0.0",
  "type": "module",
  "description": "Chrome extension for offline page indexing with embeddings",
  "main": "background.js",
  "scripts": {
    "setup:models": "node scripts/download-models.js",
    "setup:ort": "node scripts/copy-ort.js",
    "setup": "npm run setup:models && npm run setup:ort",
    "prebuild": "npm run setup:ort",
    "build": "esbuild offscreen.js --bundle --outfile=generated/offscreen-bundled.js --format=iife --target=es2020",
    "build:dev": "esbuild offscreen.js --bundle --outfile=generated/offscreen-bundled.js --format=iife --target=es2020 --sourcemap",
    "watch": "esbuild offscreen.js --bundle --outfile=generated/offscreen-bundled.js --format=iife --target=es2020 --watch",
    "format": "prettier --write \"*.js\" \"scripts/*.js\"",
    "test": "node --experimental-vm-modules --max-old-space-size=4096 node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "node --experimental-vm-modules --max-old-space-size=4096 node_modules/jest/bin/jest.js --watch",
    "test:integration": "node --experimental-vm-modules --max-old-space-size=4096 node_modules/jest/bin/jest.js tests/integration --runInBand"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "mhtml-to-html": "^2.0.0",
    "onnxruntime-web": "^1.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/chrome": "^0.1.29",
    "chrome-types": "^0.1.388",
    "esbuild": "^0.19.12",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2"
  },
  "keywords": [
    "chrome-extension",
    "embeddings",
    "offline",
    "mhtml",
    "onnx"
  ],
  "author": "",
  "license": "MIT"
}
